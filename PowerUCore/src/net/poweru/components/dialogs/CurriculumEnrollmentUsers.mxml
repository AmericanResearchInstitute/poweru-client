<?xml version="1.0" encoding="utf-8"?>
<code:CurriculumEnrollmentUsersCode
	title="Enrolled Users"
	minWidth="400"
	minHeight="500"
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:code="net.poweru.components.dialogs.code.*">
	<mx:Script>
		<![CDATA[
			import net.poweru.events.ViewEvent;
		]]>
	</mx:Script>
	<mx:DataGrid id="grid" width="100%" height="100%" dataProvider="{dp}">
		<mx:columns>
			<mx:DataGridColumn headerText="Last Name" dataField="last_name"/>
			<mx:DataGridColumn headerText="First Name" dataField="first_name"/>
		</mx:columns>
	</mx:DataGrid>
	<mx:HBox width="100%" horizontalAlign="center">
		<mx:Button label="Close" click="dispatchEvent(new ViewEvent(ViewEvent.CANCEL))"/>
	</mx:HBox>
</code:CurriculumEnrollmentUsersCode>
