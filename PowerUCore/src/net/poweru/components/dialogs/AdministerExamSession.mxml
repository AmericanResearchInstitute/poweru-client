<?xml version="1.0" encoding="utf-8"?>
<code:AdministerExamSessionCode
	minWidth="800"
	minHeight="600"
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:code="net.poweru.components.dialogs.code.*">
	<mx:Script>
		<![CDATA[
			import net.poweru.events.ViewEvent;
		]]>
	</mx:Script>
	<mx:VBox height="100%" width="100%">
		<mx:Form>
			<mx:FormItem label="Title:">
				<mx:Label text="{currentQuestions.title}"/>
			</mx:FormItem>
			<mx:FormItem label="Section:">
				<mx:Label text="{questionPoolTitle}"/>
			</mx:FormItem>
		</mx:Form>
		<mx:HRule width="100%"/>
		<mx:VBox id="questionBox" width="100%" height="100%" paddingLeft="16"/>
		<mx:HBox width="100%" horizontalAlign="center">
			<mx:Button label="Submit" click="onSubmit(event)"/>
			<mx:Button label="Cancel" click="dispatchEvent(new ViewEvent(ViewEvent.CANCEL))"/>
		</mx:HBox>
	</mx:VBox>
	
</code:AdministerExamSessionCode>
