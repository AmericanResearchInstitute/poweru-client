<?xml version="1.0" encoding="utf-8"?>
<code:ViewExamAssignmentsCode
	title="Exam Assignments"
	minWidth="600"
	minHeight="400"
	xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:code="net.poweru.components.dialogs.code.*">
	<mx:Script>
		<![CDATA[
			import net.poweru.events.ViewEvent;
		]]>
	</mx:Script>
	<mx:DataGrid id="grid" width="100%" height="100%">
		<mx:columns>
			<mx:DataGridColumn dataField="name" headerText="Name"/>
			<mx:DataGridColumn dataField="status" headerText="Status"/>
			<mx:DataGridColumn dataField="user_last_first" headerText="User"/>
		</mx:columns>
	</mx:DataGrid>
	<mx:HBox width="100%" horizontalAlign="center">
		<mx:Button label="Close" click="dispatchEvent(new ViewEvent(ViewEvent.CANCEL))"/>
	</mx:HBox>
</code:ViewExamAssignmentsCode>
